#################################
### EMAIL EXTENSION OVERRIDES ###
#################################
plugin.tx_bwemail {
  settings {
    tableOverrides {
      tx_bwbookingmanager_domain_model_entry {
        recipientAddress = FIELD:email
        recipientName = FIELD:prename FIELD:name
        subject = New booking in FIELD:calendar.name
      }
    }
  }
}
