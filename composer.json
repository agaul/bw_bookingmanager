{
	"name": "blueways/bw-bookingmanager",
	"type": "typo3-cms-extension",
	"description": "A generic bookingmanager",
	"require": {
		"blueways/bw-email": "1.*"
	},
	"autoload": {
		"psr-4": {
			"Blueways\\BwBookingmanager\\": "Classes"
		}
	},
	"require-dev": {
		"typo3/cms-base-distribution": "9.5.*",
		"bk2k/bootstrap-package": "9.1.*"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "public"
		}
	},
	"config": {
		"bin-dir": "bin",
		"vendor-dir": "vendor",
		"platform": {
			"php": "7.2"
		}
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Maik Schneider",
			"email": "m.schneider@blueways.de",
			"homepage": "http://blueways.de",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3"
	]
}
