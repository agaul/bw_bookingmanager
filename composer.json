{
	"name": "blueways/bw-bookingmanager",
	"type": "typo3-cms-extension",
	"description": "A generic bookingmanager",
	"require": {
		"blueways/bw-email": "^2.0.0"
	},
	"autoload": {
		"psr-4": {
			"Blueways\\BwBookingmanager\\": "Classes"
		}
	},
	"require-dev": {
		"typo3/cms-base-distribution": "9.5.*",
		"bk2k/bootstrap-package": "9.1.*",
		"phpmd/phpmd": "^2.6.0",
		"squizlabs/php_codesniffer": "^3.4",
		"friendsofphp/php-cs-fixer": "^2.9"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "public"
		}
	},
	"config": {
		"bin-dir": "bin",
		"vendor-dir": "vendor",
		"platform": {
			"php": "7.2"
		}
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "Maik Schneider",
			"email": "m.schneider@blueways.de",
			"homepage": "https://www.blueways.de",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3"
	]
}
