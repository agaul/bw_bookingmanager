{
	"name": "blueways/bw-bookingmanager",
	"type": "typo3-cms-extension",
	"description": "A generic bookingmanager",
	"require": {
		"blueways/bw-email": "^2.0.0",
      	"ext-json": "*"
    },
	"autoload": {
		"psr-4": {
			"Blueways\\BwBookingmanager\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Blueways\\BwBookingmanager\\Tests\\": "Tests/"
		}
	},
	"require-dev": {
		"typo3/cms-base-distribution": "9.5.*",
		"bk2k/bootstrap-package": "9.1.*",
		"phpmd/phpmd": "^2.6.0",
		"squizlabs/php_codesniffer": "^3.4",
		"friendsofphp/php-cs-fixer": "^2.9",
		"typo3/cms-lowlevel": "^9.5",
		"vimeo/psalm": "^4.0",
		"phpstan/phpstan": "^0.12",
		"typo3/testing-framework": "^4.0",
		"typo3/cms-core": "9.5.x-dev as 9.5.24",
		"typo3/cms-backend": "9.5.x-dev as 9.5.24"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "public",
			"extension-key": "bw_bookingmanager"
		}
	},
	"config": {
		"bin-dir": "bin",
		"vendor-dir": "vendor",
		"platform": {
			"php": "7.4"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "Maik Schneider",
			"email": "m.schneider@blueways.de",
			"homepage": "https://www.blueways.de",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3"
	]
}
